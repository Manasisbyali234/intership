NODE_ENV=development
PORT=5000
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/student-assistant
JWT_SECRET=your-jwt-secret-key

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key

# AWS Services
COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxx
S3_BUCKET_DOCUMENTS=student-assistant-documents
S3_BUCKET_RESUMES=student-assistant-resumes
OPENSEARCH_ENDPOINT=https://search-domain.us-east-1.es.amazonaws.com

# Bedrock
BEDROCK_MODEL_ID=anthropic.claude-v2
BEDROCK_EMBEDDING_MODEL_ID=amazon.titan-embed-text-v1

# Hugging Face API
HUGGINGFACE_API_KEY=your-huggingface-api-key
HF_TOKEN=your-hf-token